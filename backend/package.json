{
  "name": "backend",
  "version": "1.0.0",
  "description": "skindora",
  "main": "index.js",
  "scripts": {
    "dev:server": "npx nodemon",
    "dev:docs": "npx nodemon --watch src/docs --ext yaml --exec \"npm run docs:bundle\"",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:docs\"",
    "build": "rimraf ./dist && tsc && tsc-alias && swagger-cli bundle ./src/docs/openapi.yaml --outfile ./dist/openapi.json --type json",
    "start": "node dist/index.js",
    "docs:bundle": "swagger-cli bundle ./src/docs/openapi.yaml --outfile ./public/openapi.json --type json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/config": "^3.3.5",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.2",
    "@types/lodash": "^4.17.17",
    "@types/moment": "^2.11.29",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.19",
    "@types/nodemailer": "^6.4.17",
    "@types/qs": "^6.14.0",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "swagger-cli": "^4.0.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.9",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "cron": "^4.3.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "mongodb": "^6.16.0",
    "multer": "^2.0.0",
    "nodemailer": "^7.0.3",
    "qs": "^6.14.0",
    "redis": "^5.1.0",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.6.1",
    "uuid": "^11.1.0",
    "vnpay": "^2.3.0",
    "yamljs": "^0.3.0"
  }
}
